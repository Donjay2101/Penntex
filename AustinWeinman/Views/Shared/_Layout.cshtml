<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/Gridmvc.css" rel="stylesheet" />    
    <script src="~/Scripts/jquery.js"></script>
    <script src="~/Scripts/gridmvc.min.js"></script>
    <script src="~/Scripts/jquery-ui.min.js"></script>
    <script src="~/Scripts/common.js"></script>
    <link href="~/Content/jquery-ui.min.css" rel="stylesheet" />

    <script>
        $(document).ready(function () {

            //debugger;
            $(".date").each(function () {
                //debugger;
                //  var j= $.noConflict();
                $(this).datepicker();
            });

        });
    </script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="/Projects/Index">
                    <img src="~/Images/logo.png" /> 
                </a>
                
                @*@Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Project", "Index", "Projects")</li>
                    <li>@Html.ActionLink("Agreement Of Sale", "Index", "Agreementofsales")</li>
                    <li>@Html.ActionLink("Lease", "Index", "Leases")</li>
                    <li>@Html.ActionLink("Loans", "Index", "Loans")</li>
                    <li>@Html.ActionLink("Sellers", "Index", "Sellers")</li>
                    <li>@Html.ActionLink("Staff", "Index", "Staffs")</li>
                    <li>@Html.ActionLink("Tenant", "Index", "Tenants")</li>
                    <li>@Html.ActionLink("Lender", "Index", "Lenders")</li>
                    <li>@Html.ActionLink("Vendors", "Index", "Vendors")</li>
                    <li>@Html.ActionLink("Entity", "Index", "Entities")</li>
                    <li>@Html.ActionLink("Architect", "Index", "Architects")</li>
                    <li>@Html.ActionLink("Construction Type", "Index", "ConstructionTypes")</li>
                             </ul>
                @*@Html.Partial("_LoginPartial")*@
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
