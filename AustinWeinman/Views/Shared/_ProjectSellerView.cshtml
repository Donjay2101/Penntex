
@model IEnumerable<AustinWeinman.Models.Seller>
@using GridMvc.Html




@Html.Grid(Model).Columns(columns =>
{
    columns.Add(c => c.FirstName).Titled("First Name");
    columns.Add(c => c.LastName).Titled("Last Name");
    columns.Add(c => c.Company).Titled("Company");
    columns.Add(c => c.Jobtitle).Titled("Job Title");
    columns.Add(c => c.WorkPhone).Titled("Work Phone");
   
}).WithPaging(25).Sortable().Filterable().WithMultipleFilters()
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/gridmvc.min.js"></script>



