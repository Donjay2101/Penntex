@model IEnumerable<AustinWeinman.ViewModel.AgreementofsalesViewModel>
@using GridMvc.Html
@Html.Grid(Model).Columns(columns =>
{    
    columns.Add(c => c.SellersName).Titled("Seller");
    columns.Add(c => c.PurchasePrice).Titled("Price");   
    columns.Add(c => c.NextPayment).Titled("Next Payment");
    columns.Add(c => c.AOSEffectiveDate).Titled("AOS Effective Date");    
}).WithPaging(25).Sortable().Filterable().WithMultipleFilters()
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/gridmvc.min.js"></script>


