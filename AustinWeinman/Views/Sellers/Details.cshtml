@model AustinWeinman.Models.Seller

@{
    ViewBag.Title = "Details";
}



<div class="row">
    <div class="col-md-12">

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">
                <div class="row">
                    <div class="col-md-4 col-md-offset-8">
                        <input type="button" value="Back to List" class="linkbtn  pull-right" onclick="goToPrevious('@ViewBag.ReturnUrl')" />
                    </div>
                </div>
                <hr />
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                @Html.HiddenFor(model => model.ID)
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-4">First Name:</label>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.FirstName, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Last Name:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.LastName, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Company:</label>

                            <div class="col-md-8">

                                @Html.DisplayFor(model => model.Company, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Email:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Email, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Job Title:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Jobtitle, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Work Phone:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.WorkPhone, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Home Phone:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.HomePhone, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Mobile Phone:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.MobilePhone, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Address 1:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Address1, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-4">Address 2:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Address2, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">City:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.City, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">State:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.State, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Zip Postal:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.ZIPPostal, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Country:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Country, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Webpage::</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Webpage, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Notes:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Notes, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Groups:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Groups, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Project Name:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.ProjectName, new { @class = "form-control displayname" })


                            </div>
                        </div>
                    </div>
                </div>

            </div>
        }





    </div>
</div>
