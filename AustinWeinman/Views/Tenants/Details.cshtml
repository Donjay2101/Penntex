@model AustinWeinman.Models.Tenant
@{
    ViewBag.Title = "Details";
}


<div class="row">
    <div class="col-md-12">



        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">
                <div class="row">
                    <div class="col-md-4 col-md-offset-8">
                      <input type="button" value="Back to List" class="linkbtn  pull-right" onclick="goToPrevious('@ViewBag.ReturnUrl')" />
                    </div>
                </div>
                <hr />
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                @Html.HiddenFor(model => model.ID)
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-4">Company Name:</label>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.CompanyName, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Contact Person:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.ContactPerson, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Email:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Email, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Contact Phone:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.ContactPhone, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Address:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Address, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">City:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.City, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                    </div>
                    <div class="col-md-6">

                        <div class="form-group">
                            <label class="control-label col-md-4">State:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.State, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Zip Code:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.ZipCode, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Country:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Country, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Webpage:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Webpage, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">Notes:</label>

                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.Notes, new { htmlAttributes = new { @class = "form-control displayname" } })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }

    </div>
</div>
